local Toggle = Tabs.Iamrich:AddToggle("STRENGTH", {Title = "auto playtime pass only free", Default = false})

local isRunning = false

-- Listen for the toggle change
Toggle:OnChanged(function(state)
    if state then
        -- Start the loop when the toggle is on
        if not isRunning then
            isRunning = true
            task.spawn(function()  -- Run in a separate thread to prevent freezing
                while isRunning do
                    -- Loop through and claim rewards
                    for i = 1, 12 do
                        local args = {
                            [1] = "Free",
                            [2] = i
                        }

                        game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit")
                            :WaitForChild("Services"):WaitForChild("EventPassService")
                            :WaitForChild("RF"):WaitForChild("ClaimReward")
                            :InvokeServer(unpack(args))
                    end

                    -- Call the reset function after claiming all rewards
                    game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit")
                        :WaitForChild("Services"):WaitForChild("EventPassService")
                        :WaitForChild("RF"):WaitForChild("Reset")
                        :InvokeServer()

                    -- Sleep for a short period to prevent overwhelming the server
                    wait(1)  -- Adjust as needed
                end
            end)
        end
    else
        -- Stop the loop when the toggle is off
        isRunning = false
    end
end)

